<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="dogai">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import dogai urdf file -->
    <xacro:include filename="$(find dogai)/urdf/dogai.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="dogai.ros2_control.xacro" />


    <xacro:dogai_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
